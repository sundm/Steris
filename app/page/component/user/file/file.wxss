/* page/component/user/file/file.wxss */
.main{
  margin: 25rpx 25rpx 30rpx 25rpx;
}
page{
  background-color:rgb(241, 248, 248);
}
.over{
  clear: both;
  height: 0;
}
.top{
  width: 100%;
  height: auto;
}
.top-title{
  padding-left: 5rpx;
  padding-top: 5rpx;
  border-radius: 8px;
  background-color: white;
  height: 60rpx;
  margin: 15rpx 0 0 0;
}
.top-title-image{
  padding: 5rpx;
  width: 40rpx;
  height: 40rpx;
  float: left;
  overflow: hidden;
}
.top-title image{
  width: 100%;
  height: 100%;
}


.top-body-table{
  background-color: white;
  border-radius: 8px;
  width: 24%;
  text-align: center;
  font-size: 25rpx;
  float: left;
  margin-left: 0.8%;
  margin-top: 10rpx;
}
.top-body-table image{
  width: 100rpx;
  height: 100rpx;
}

.button{
  overflow: hidden;
}
.kong{
  width: 100%;
  text-align: center;
}
.name{
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  align-content: center;
}